@model Item
@{
    ViewData["Title"] = Model.ProductName;
}

<h3>@Model.ProductName</h3>

<p>
    Item Id: @Model.ItemId<br />
    Price: @Model.Price<br />
    Quantity Available: @Model.QuantityAvailable<br />
</p>
